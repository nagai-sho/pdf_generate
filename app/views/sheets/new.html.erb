<div class="sheet">
  <div>New</div>
  <%= link_to 'Index', root_path %>
  <%= form_with model: @sheet, local: true do |f| %>
    <%= f.label :content, 'input_form' %><br>
    <%= f.text_field :content %><br>
    <%= f.submit 'send' %>
  <% end %>
  <%= link_to 'PDF', new_pdf_sheets_path, target: '_blank', class: 'btn btn-primary' %>
</div>